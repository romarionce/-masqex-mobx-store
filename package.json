{
  "name": "mobx-one-line",
  "module": "src/index.ts",
  "type": "module",
  "version": "0.0.4",
  "description": "Create Mobx Store in one line",
  "license": "MIT",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun build --target=node ./src/index.ts --outfile=dist/index.js && bun run build:declaration",
    "build:declaration": "tsc --emitDeclarationOnly --project tsconfig.types.json"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/react": "^18.3.3",
    "react": "^18.3.1"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}
